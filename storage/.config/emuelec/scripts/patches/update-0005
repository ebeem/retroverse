# override emuelec config path with the emuelec_overrides dictionary
def configure_cores():
    for core in cores_overrides:
        config_path = "/storage/.config/retroarch/config/{}'/{}.opt".format(core, core)
        with open(config_path, "r") as f:
            lines = f.readlines()
            for i in range(len(lines)):
                for override in cores_overrides[core]:
                    if lines[i].startswith(override):
                        lines[i] = (
                            override + " = " + str(cores_overrides[override]) + "\n"
                        )

        with open(config_path, "w") as f:
            f.writelines(lines)


configure_cores()
print("configs updated")
